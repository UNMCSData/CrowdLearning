function showNav(){ $(".subnav").slideDown();}
function hideNav(){ $(".subnav").slideUp();}

function moveHomeMenu() {
    currentSize = $(window).width();
        if (currentSize < breakpt) {
                $('#home-nav').detach().appendTo('#mobile-menu');
                sizeStatus = 'small';   
        } else if (currentSize > breakpt) {
                        $('#home-nav').detach().appendTo('#nav .container');
                        sizeStatus = 'big';
        }
}
